# PulseGuard Environment Variables

# API Keys
OPENAI_API_KEY=your_openai_api_key_here
WHO_API_KEY=your_who_api_key_here
CDC_API_KEY=your_cdc_api_key_here

# Database Configuration
DATABASE_URL=your_database_connection_string_here
DATABASE_USER=your_database_username
DATABASE_PASSWORD=your_database_password

# Server Configuration
PORT=3001
NODE_ENV=development

# Data Pipeline Configuration
SCRAPE_INTERVAL=21600  # 6 hours in seconds
RISK_REPORT_INTERVAL=86400  # 24 hours in seconds

# Map Provider
MAP_PROVIDER_API_KEY=your_map_provider_api_key_here

# Feature Flags
ENABLE_AI_PREDICTIONS=true
ENABLE_REAL_TIME_ALERTS=true
ENABLE_DATA_SCRAPING=true

# Notification Services
EMAIL_SERVICE_API_KEY=your_email_service_api_key_here
SMS_SERVICE_API_KEY=your_sms_service_api_key_here

# Logging
LOG_LEVEL=info